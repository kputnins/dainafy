{
  "name": "@dainafy/model",
  "version": "0.0.1",
  "description": "A LSTM model that generates Latvian folk songs - dainas",
  "main": "dist/common/index.js",
  "module": "dist/esnext/index.js",
  "types": "dist/esnext/index.d.ts",
  "repository": "",
  "author": "Kaspars Putniņš",
  "license": "MIT",
  "scripts": {
    "start": "ts-node-transpile-only src/index.ts",
    "clean": "rm -rf dist",
    "build": "yarn clean && yarn transpile:esnext && yarn transpile:common",
    "transpile:esnext": "tsc --project tsconfig.esnext.json",
    "transpile:common": "tsc --project tsconfig.common.json",
    "lint": "yarn lint:ts && yarn lint:eslint",
    "lint:eslint": "eslint --fix \"./src/**/*.{js,jsx,ts,tsx,json}\"",
    "lint:ts": "tsc --project ./tsconfig.json",
    "format": "prettier --write \"./src/**/*.{js,jsx,ts,tsx,json,css,scss,md}\""
  },
  "devDependencies": {
    "@liene-putnina/eslint-config-lintmyride": "^1.0.8",
    "@typescript-eslint/eslint-plugin": "^5.37.0",
    "@typescript-eslint/parser": "^5.37.0",
    "eslint": "^8.23.1",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-typescript": "^3.5.1",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.6.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.31.8",
    "eslint-plugin-react-hooks": "^4.6.0",
    "prettier": "^2.7.1",
    "ts-node": "^10.9.1",
    "typescript": "^4.8.4"
  }
}
